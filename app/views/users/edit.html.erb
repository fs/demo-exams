<h1>Your settings</h1>
<% form_for @user do |form| %>
  <%= render :partial => 'form', :object => form %>
  <div>
    <button class="save" type="submit"><%= image_tag("save.png") %>Save</button>
    <% link_to(root_path, :class => 'button cancel') do %>
      <%= image_tag('cancel.png') %>
      Cancel
    <% end %>
  </div>
<% end %>